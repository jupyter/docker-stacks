"db.git/commit/?id=8197d7824f814c5d4b992b4c8730b5b0f7ec589a RUN echo "
"\"MANPATH_MAP ${CONDA_DIR}/bin ${CONDA_DIR}/man\" >> /etc/manpath.config "
"\\"
